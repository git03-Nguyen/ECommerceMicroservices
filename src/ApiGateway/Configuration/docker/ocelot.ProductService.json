{
  "Routes": [
    {
      "SwaggerKey": "productservice",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6300
        }
      ],
      "UpstreamPathTemplate": "/ProductService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ],
      "DownstreamPathTemplate": "/{everything}",
      "FileCacheOptions": {
        "TtlSeconds": 10,
        "Region": "Products"
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      }
    }
  ]
}