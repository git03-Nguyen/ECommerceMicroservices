{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:6000"
  },
  "Routes": [
    {
      "SwaggerKey": "authenservice",
      "UpstreamPathTemplate": "/AuthenService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "15s",
        "PeriodTimespan": 1,
        "Limit": 10
      },
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6001
        }
      ]
    },
    {
      "SwaggerKey": "customerservice",
      "UpstreamPathTemplate": "/CustomerService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6002
        }
      ]
    },
    {
      "SwaggerKey": "productservice",
      "UpstreamPathTemplate": "/ProductService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6003
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10,
        "Region": "Products"
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "authenservice",
      "Config": [
        {
          "Name": "AuthenService API",
          "Version": "v1",
          "Url": "http://localhost:6001/swagger/swagger.json"
        }
      ]
    },
    {
      "Key": "customerservice",
      "Config": [
        {
          "Name": "CustomerService API",
          "Version": "v1",
          "Url": "http://localhost:6002/swagger/swagger.json"
        }
      ]
    },
    {
      "Key": "productservice",
      "Config": [
        {
          "Name": "ProductService API",
          "Version": "v1",
          "Url": "http://localhost:6003/swagger/swagger.json"
        }
      ]
    }
  ]
}