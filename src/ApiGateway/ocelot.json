{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:6000"
  },
  "Routes": [
    {
      "SwaggerKey": "authenservice",
      "UpstreamPathTemplate": "/AuthenService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "15s",
        "PeriodTimespan": 1,
        "Limit": 10
      },
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6100
        }
      ]
    },
    {
      "SwaggerKey": "customerservice",
      "UpstreamPathTemplate": "/CustomerService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6200
        }
      ]
    },
    {
      "SwaggerKey": "productservice",
      "UpstreamPathTemplate": "/ProductService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ],
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 6300
        }
      ],
      "FileCacheOptions": {
        "TtlSeconds": 10,
        "Region": "Products"
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      }
    },
    {
      "SwaggerKey": "cartservice",
        "UpstreamPathTemplate": "/CartService/{everything}",
        "UpstreamHttpMethod": [
          "Get",
          "Post"
        ],
        "DownstreamPathTemplate": "/{everything}",
        "DownstreamScheme": "http",
        "DownstreamHostAndPorts": [
          {
            "Host": "host.docker.internal",
            "Port": 6400
          }
        ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "authenservice",
      "Config": [
        {
          "Name": "AuthenService API",
          "Version": "v1",
          "Url": "http://localhost:6100/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "customerservice",
      "Config": [
        {
          "Name": "CustomerService API",
          "Version": "v1",
          "Url": "http://localhost:6200/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "productservice",
      "Config": [
        {
          "Name": "ProductService API",
          "Version": "v1",
          "Url": "http://localhost:6300/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "cartservice",
      "Config": [
        {
          "Name": "CartService API",
          "Version": "v1",
          "Url": "http://localhost:6400/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}