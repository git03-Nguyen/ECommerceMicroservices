{
  "Routes": [
    {
      "SwaggerKey": "basket.service",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 16200
        }
      ],
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "UpstreamPathTemplate": "/BasketService/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Patch",
        "Delete"
      ],
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "15s",
        "PeriodTimespan": 1,
        "Limit": 20
      }
    },
    {
      "Key": "basket",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 16200
        }
      ],
      "DownstreamPathTemplate": "/api/v1/Basket/Get",
      "UpstreamPathTemplate": "/BasketService/Basket/Get",
      "UpstreamHttpMethod": [
        "Get"
      ]
    }
  ]
}